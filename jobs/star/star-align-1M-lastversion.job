#!/bin/bash
NUM_THREAD_PARAM=$1
DATASET_PARAM=$2
bsub -M 64000 -n $NUM_THREAD_PARAM \
        -J star-align-1M-lastversion_job \
        -e star-align-1M-lastversion_job.%J.err \
        -o star-align-1M-lastversion_job.%J.out \
        sh star-align-1M-lastversion.sh $NUM_THREAD_PARAM $DATASET_PARAM
