#!/bin/bash
NUM_THREAD_PARAM=$1
DATASET_PARAM=$2
bsub -M 200000 -n $NUM_THREAD_PARAM \
        -J contextmap2-align-noannotation_job \
        -e contextmap2-align-noannotation_job.%J.err \
        -o contextmap2-align-noannotation_job.%J.out \
        sh contextmap2-align-noannotation.sh $NUM_THREAD_PARAM $DATASET_PARAM
